{
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/next"
    }
  ],
  "crons": [
    {
      "path": "/api/cron/collect-events",
      "schedule": "0 2 * * *"
    },
    {
      "path": "/api/cron/collect-events?type=deep",
      "schedule": "0 3 * * 0"
    }
  ]
}

